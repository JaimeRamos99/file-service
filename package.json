{
  "name": "core-service",
  "version": "1.0.0",
  "description": "core backend service",
  "main": "index.ts",
  "scripts": {
    "build": "npx tsc",
    "start": "doppler run --command='npm run build && node build/index.js'",
    "dev": "doppler run --command='nodemon'",
    "lint:check": "npx prettier . --check",
    "lint:write": "npx prettier . --write",
    "db:make-migration": "doppler run --command='npx knex migrate:make --knexfile=./src/db/knexfile.ts $1'",
    "db:migrate": "doppler run --command='npx knex migrate:latest --knexfile=./src/db/knexfile.ts'",
    "db:migration-status": "doppler run --command='npx knex migrate:status --knexfile=./src/db/knexfile.ts --env=development'",
    "db:rollback": "doppler run --command='npx knex migrate:rollback --knexfile=./src/db/knexfile.ts'",
    "db:make-seed": "doppler run --command='npx knex seed:make $npm_config_seedname --knexfile=./src/db/knexfile.ts'",
    "db:run-seed": "doppler run --command='npx knex seed:run --knexfile=./src/db/knexfile.ts'"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/JaimeRamos99/travel.git"
  },
  "keywords": [
    "trip",
    "plan",
    "travel",
    "pdf"
  ],
  "author": "Jaime Ramos Rossetes",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/JaimeRamos99/travel/issues"
  },
  "homepage": "https://github.com/JaimeRamos99/travel#readme",
  "devDependencies": {
    "@types/express": "^4.17.21",
    "@types/knex": "^0.16.1",
    "@types/multer": "^1.4.12",
    "@types/node": "^22.3.0",
    "@types/uuid": "^10.0.0",
    "nodemon": "^3.1.4",
    "prettier": "3.3.3",
    "ts-node": "^10.9.2",
    "typescript": "^5.5.4"
  },
  "dependencies": {
    "@google-cloud/documentai": "^8.9.0",
    "@google-cloud/storage": "^7.12.1",
    "envalid": "^8.0.0",
    "express": "^4.19.2",
    "knex": "^3.1.0",
    "multer": "^1.4.5-lts.1",
    "pg": "^8.12.0",
    "uuid": "^10.0.0"
  }
}
